{
  "version": "0.2.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsc --project tsconfig.build.json",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "@coopersystem-fsd/typeorm-naming-strategy",
  "author": "Bc05",
  "module": "dist/typeorm-naming-strategy.esm.js",
  "size-limit": [
    {
      "path": "dist/typeorm-naming-strategy.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/typeorm-naming-strategy.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.6.0",
    "husky": "^4.3.0",
    "pg": "^8.4.2",
    "size-limit": "^4.6.0",
    "sqlite3": "^5.0.0",
    "tsdx": "^0.14.0",
    "tslib": "^2.0.1",
    "typeorm": "^0.2.28",
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "typeorm-naming-strategies": "^2.0.0"
  }
}
